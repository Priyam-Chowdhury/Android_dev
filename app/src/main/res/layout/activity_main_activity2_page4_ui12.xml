<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2_page4_ui1_2">

    <FrameLayout
        android:id="@+id/frameLayout4"
        android:layout_width="409dp"
        android:layout_height="579dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="151dp">

        <Button
            android:id="@+id/button158"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Description"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="150dp"
            app:backgroundTint="#333645" />

        <TextView
            android:id="@+id/textView60"
            android:layout_width="380dp"
            android:layout_height="wrap_content"
            android:text="The Victoria Memorial is an iconic and majestic monument located in Kolkata, India. It is one of the most famous and prominent landmarks of the city, showcasing a blend of architectural splendor, historical significance, and cultural heritage. The memorial is dedicated to Queen Victoria, the Empress of India during the British colonial era."
            android:translationX="6dp"
            android:translationY="200dp" />

        <Button
            android:id="@+id/button161"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Review"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="310dp" />

        <RatingBar
            android:id="@+id/ratingBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="6dp"
            android:translationY="360dp" />
    </FrameLayout>

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/frameLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/victoria_memorial_new" />
</androidx.constraintlayout.widget.ConstraintLayout>