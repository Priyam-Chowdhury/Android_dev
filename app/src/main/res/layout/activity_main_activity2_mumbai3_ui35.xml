<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2_mumbai3_ui3_5">

    <ImageView
        android:id="@+id/imageView144"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/girghaum_chowpatty_new" />

    <FrameLayout
        android:layout_width="409dp"
        android:layout_height="579dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="151dp">

        <Button
            android:id="@+id/button385"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Description"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="150dp" />

        <TextView
            android:id="@+id/textView175"
            android:layout_width="380dp"
            android:layout_height="wrap_content"
            android:text=" Girgaum Chowpatty, often simply referred to as Chowpatty, is a famous public beach located in South Mumbai, India. It is a popular destination for both locals and tourists and is known for its vibrant atmosphere, cultural events, and delicious street food. "
            android:translationX="6dp"
            android:translationY="200dp" />

        <Button
            android:id="@+id/button388"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Review"
            android:textSize="16sp"
            android:translationX="6dp"
            android:translationY="295dp" />

        <RatingBar
            android:id="@+id/ratingBar117"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="6dp"
            android:translationY="345dp" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>