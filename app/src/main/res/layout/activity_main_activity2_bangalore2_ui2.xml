<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2_bangalore2_ui2">

    <FrameLayout
        android:layout_width="409dp"
        android:layout_height="100dp"
        android:backgroundTint="#333645"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <ImageView
            android:id="@+id/imageView165"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/texture_6" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="409dp"
        android:layout_height="628dp"
        android:background="#FCFD95"
        android:backgroundTint="#FCFD95"
        android:translationY="100dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="102dp">

        <Button
            android:id="@+id/button142"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Tour Description"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp" />

        <TextView
            android:id="@+id/textView50"
            android:layout_width="395dp"
            android:layout_height="136dp"
            android:text="We're thrilled to have you continue this journey with us as we delve deeper into the beauty and charm that Bangalore has to offer. Yesterday was just a glimpse of what this city holds, and today promises to be equally exciting and enriching.  We have a captivating lineup of activities planned for you today. From exploring the city's modern architectural marvels to indulging in its artistic and cultural treasures, you're in for a treat."
            android:translationX="6dp"
            android:translationY="50dp" />

        <Button
            android:id="@+id/button143"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Tour Itinerary"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="180dp" />

        <TextView
            android:id="@+id/textView51"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cubbon Park\nBangalore Aquarium\nBlossom Book House\nCauvery Emporium\nVisvesvaraya Industrial and Technological Museum\nM G Road"
            android:translationX="6dp"
            android:translationY="230dp" />

        <Button
            android:id="@+id/button144"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Photos"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="350dp" />

        <androidx.gridlayout.widget.GridLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="55dp"
            android:translationX="10dp"
            android:translationY="420dp"
            app:columnCount="3"
            app:rowCount="5">

            <ImageButton
                android:id="@+id/imageButton109"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_column="0"
                app:layout_row="0"
                app:srcCompat="@drawable/cubbon_park" />

            <ImageButton
                android:id="@+id/imageButton115"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_column="1"
                app:layout_row="0"
                app:srcCompat="@drawable/bangalore_aquarium" />

            <ImageButton
                android:id="@+id/imageButton116"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_column="2"
                app:layout_row="0"
                app:srcCompat="@drawable/blossom_book_house" />

            <ImageButton
                android:id="@+id/imageButton117"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_column="0"
                app:layout_row="1"
                app:srcCompat="@drawable/cauvery_emporium" />

            <ImageButton
                android:id="@+id/imageButton118"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_column="1"
                app:layout_row="1"
                app:srcCompat="@drawable/visvesvaraya_industrial_and_technological_museum" />

            <ImageButton
                android:id="@+id/imageButton119"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_column="2"
                app:layout_row="1"
                app:srcCompat="@drawable/mg_road_bangalore" />
        </androidx.gridlayout.widget.GridLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>