<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2_page5_ui2_3">

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/shobhabazar_rajbari_kolkata_new" />

    <FrameLayout
        android:layout_width="409dp"
        android:layout_height="579dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="151dp">

        <Button
            android:id="@+id/button175"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Description"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="150dp" />

        <TextView
            android:id="@+id/textView69"
            android:layout_width="380dp"
            android:layout_height="wrap_content"
            android:text=" The Sovabazar Rajbari, also known as the Sovabazar Palace, is a historic mansion located in the Sovabazar area of Kolkata, West Bengal, India. It is an architectural gem that reflects the grandeur and cultural heritage of the region during the colonial era. The Rajbari was the residence of the influential Tagore family, who played a significant role in shaping the cultural and social landscape of Kolkata."
            android:translationX="6dp"
            android:translationY="200dp" />

        <Button
            android:id="@+id/button176"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#333645"
            android:text="Review"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationX="6dp"
            android:translationY="325dp" />

        <RatingBar
            android:id="@+id/ratingBar10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="6dp"
            android:translationY="375dp" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>